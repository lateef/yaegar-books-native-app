{
  "name": "yaegar-books-native-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-preset-react-native-stage-0": "^1.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "fetch-mock": "^6.4.2",
    "jest": "^22.4.3",
    "jest-react-native": "^18.0.0",
    "jsdom-global": "^3.0.2",
    "react-test-renderer": "16.3.1",
    "redux-mock-store": "^1.5.1"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jestsetup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/e2e/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|expo|react-navigation|native-base-shoutem-theme|@shoutem/theme|@shoutem/animation|@shoutem/ui|tcomb-form-native|native-base)"
    ]
  },
  "dependencies": {
    "native-base": "^2.4.4",
    "react": "16.3.1",
    "react-dom": "^16.3.2",
    "react-native": "~0.55.2",
    "react-native-navigation": "^1.1.462",
    "react-native-vector-icons": "^4.6.0",
    "react-redux": "^5.0.7",
    "realm": "^2.6.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.1",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.2.1"
  }
}
